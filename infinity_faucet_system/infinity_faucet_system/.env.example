# Environment template for Infinity X One
# Copy this file to `.env` and fill in your own values.  The API
# server will read these variables at runtime via os.environ.

# Supabase connection
SUPABASE_URL=https://your-supabase-url.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key

# API ports
PRIMARY_PORT=8000
FALLBACK_PORT=8001

# Target daily profit (used by PickyBot heuristics)
TARGET_PROFIT_PER_DAY=3000

# Cluster name (for multi cluster deployments, e.g. golden or k8)
CLUSTER_NAME=golden